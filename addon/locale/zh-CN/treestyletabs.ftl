treestyletabs-title = 树形标签页
treestyletabs-menu-prefs = 树形标签页设置
treestyletabs-menu-toggle = 切换树形标签页侧边栏

treestyletabs-context-close = 关闭标签页
treestyletabs-context-close-tree = 关闭标签页及子项
treestyletabs-context-collapse = 折叠树
treestyletabs-context-expand = 展开树
treestyletabs-context-collapse-all = 全部折叠
treestyletabs-context-expand-all = 全部展开
treestyletabs-context-make-root = 设为根标签页
treestyletabs-context-move-up = 上移
treestyletabs-context-move-down = 下移

treestyletabs-pref-title = 树形标签页设置
treestyletabs-pref-display = 显示设置
treestyletabs-pref-position = 侧边栏位置
treestyletabs-pref-position-left = 左侧
treestyletabs-pref-position-right = 右侧
treestyletabs-pref-width = 侧边栏宽度 (px)
treestyletabs-pref-indent = 缩进大小 (px)
treestyletabs-pref-behavior = 行为
treestyletabs-pref-autocollapse = 展开时自动折叠其他树
treestyletabs-pref-collapseonclose = 关闭父项时折叠子项
treestyletabs-pref-showclose = 显示关闭按钮

treestyletabs-empty = 没有打开的标签页
treestyletabs-tooltip-collapse = 点击折叠
treestyletabs-tooltip-expand = 点击展开
